{
  "name": "@gjuchault/typescript-service-sdk",
  "version": "0.0.0-development",
  "description": "SDK library for https://github.com/gjuchault/typescript-service-starter",
  "keywords": [
    "typescript",
    "service",
    "sdk"
  ],
  "homepage": "https://github.com/gjuchault/typescript-service-sdk",
  "bugs": "https://github.com/gjuchault/typescript-service-sdk/issues",
  "author": "Gabriel Juchault <gabriel.juchault@gmail.com>",
  "repository": "gjuchault/typescript-service-sdk",
  "type": "module",
  "exports": "./build/index.js",
  "types": "./build/index.d.ts",
  "license": "MIT",
  "engines": {
    "node": "^18.17.1",
    "npm": "^9.6.7"
  },
  "volta": {
    "node": "18.17.1",
    "npm": "9.6.7"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run clean && npm run type:dts && npm run build:main",
    "build:main": "tsx ./scripts/build.ts",
    "clean": "rimraf build coverage nyc_output",
    "type:dts": "tsc --emitDeclarationOnly --project tsconfig.build.json",
    "type:check": "tsc --noEmit",
    "format": "prettier \"src/**/*.ts\" --write",
    "format:check": "prettier \"src/**/*.ts\" --check",
    "lint": "eslint src --ext .ts --fix",
    "lint:check": "eslint src --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "spell:check": "cspell \"{README.md,CODE_OF_CONDUCT.md,CONTRIBUTING.md,.github/*.md,src/**/*.ts}\"",
    "cz": "cz",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@ryansonshine/commitizen": "^4.2.8",
    "@ryansonshine/cz-conventional-changelog": "^3.3.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^10.0.4",
    "@semantic-release/github": "^9.0.7",
    "@semantic-release/npm": "^10.0.6",
    "@semantic-release/release-notes-generator": "^11.0.7",
    "@types/ms": "^0.7.31",
    "@types/node": "^18.16.3",
    "@types/prompts": "^2.4.5",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "c8": "^8.0.1",
    "cspell": "^7.3.6",
    "esbuild": "^0.19.5",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "globby": "^13.2.2",
    "nyc": "^15.1.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "semantic-release": "^21.1.2",
    "source-map-support": "^0.5.21",
    "typescript": "^5.2.2",
    "tsx": "^3.13.0",
    "vitest": "^0.34.5"
  },
  "peerDependencies": {
    "@autotelic/fastify-opentelemetry": "^0.17.1",
    "@fastify/circuit-breaker": "^3.2.0",
    "@fastify/cookie": "^9.0.4",
    "@fastify/cors": "^8.3.0",
    "@fastify/etag": "^4.2.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/helmet": "^11.0.0",
    "@fastify/multipart": "^7.7.3",
    "@fastify/rate-limit": "^8.0.3",
    "@fastify/under-pressure": "^8.3.0",
    "@opentelemetry/api": "^1.4.1",
    "@opentelemetry/core": "^1.15.2",
    "@opentelemetry/exporter-prometheus": "^0.41.2",
    "@opentelemetry/resources": "^1.15.2",
    "@opentelemetry/sdk-node": "^0.41.2",
    "@opentelemetry/sdk-trace-base": "^1.15.2",
    "@opentelemetry/semantic-conventions": "^1.15.2",
    "@ts-rest/core": "^3.28.0",
    "@ts-rest/fastify": "^3.28.0",
    "@ts-rest/open-api": "^3.28.0",
    "bullmq": "^4.7.1",
    "fastify": "^4.21.0",
    "fastify-plugin": "^4.5.1",
    "http-terminator": "^3.2.0",
    "ioredis": "^5.3.2",
    "ms": "^2.1.3",
    "neverthrow": "^6.0.0",
    "pino": "^8.15.0",
    "slonik": "^34.1.0",
    "ts-pattern": "^5.0.5",
    "umzug": "^3.3.1",
    "zod": "^3.21.4"
  }
}
