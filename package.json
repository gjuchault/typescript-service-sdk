{
  "name": "@gjuchault/typescript-service-sdk",
  "version": "0.0.0-development",
  "description": "SDK library for https://github.com/gjuchault/typescript-service-starter",
  "keywords": [
    "typescript",
    "service",
    "sdk"
  ],
  "homepage": "https://github.com/gjuchault/typescript-service-sdk",
  "bugs": "https://github.com/gjuchault/typescript-service-sdk/issues",
  "author": "Gabriel Juchault <gabriel.juchault@gmail.com>",
  "repository": "gjuchault/typescript-service-sdk",
  "type": "module",
  "exports": "./build/index.js",
  "types": "./build/index.d.ts",
  "license": "MIT",
  "engines": {
    "node": "^21.1.0",
    "npm": "^10.2.0"
  },
  "volta": {
    "node": "21.1.0",
    "npm": "10.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run clean && npm run type:dts && npm run build:src",
    "build:src": "tsx ./scripts/build.ts",
    "clean": "rimraf build coverage",
    "type:dts": "tsc --emitDeclarationOnly --project tsconfig.build.json",
    "type:check": "tsc --noEmit",
    "format": "prettier \"src/**/*.ts\" --write",
    "format:check": "prettier \"src/**/*.ts\" --check",
    "lint": "eslint src --ext .ts --fix",
    "lint:check": "eslint src --ext .ts",
    "test": "tsx --test src/**/*.test.ts",
    "test:watch": "tsx --watch --test src/**/*.test.ts",
    "test:coverage": "npm run clean && PORT=1987 LOG_LEVEL=error NODE_V8_COVERAGE=./coverage c8 -r html tsx --experimental-test-coverage --test \"src/**/*.test.ts\"",
    "spell:check": "cspell \"{README.md,CODE_OF_CONDUCT.md,CONTRIBUTING.md,.github/*.md,src/**/*.ts}\"",
    "cz": "cz",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@ryansonshine/commitizen": "^4.2.8",
    "@ryansonshine/cz-conventional-changelog": "^3.3.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/github": "^9.2.3",
    "@semantic-release/npm": "^11.0.1",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@types/ms": "^0.7.34",
    "@types/node": "^18.16.3",
    "@types/prompts": "^2.4.8",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "@vitest/coverage-v8": "^0.34.6",
    "c8": "^8.0.1",
    "cspell": "^8.0.0",
    "esbuild": "^0.19.5",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "globby": "^13.2.2",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "semantic-release": "^22.0.8",
    "source-map-support": "^0.5.21",
    "typescript": "^5.2.2",
    "tsx": "^3.14.0"
  },
  "peerDependencies": {
    "@fastify/circuit-breaker": "^3.2.0",
    "@fastify/cookie": "^9.1.0",
    "@fastify/cors": "^8.4.0",
    "@fastify/etag": "^4.2.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.0.0",
    "@fastify/rate-limit": "^8.0.3",
    "@fastify/under-pressure": "^8.3.0",
    "@opentelemetry/api": "^1.6.0",
    "@opentelemetry/core": "^1.17.1",
    "@opentelemetry/exporter-prometheus": "^0.44.0",
    "@opentelemetry/resources": "^1.17.1",
    "@opentelemetry/sdk-node": "^0.44.0",
    "@opentelemetry/sdk-trace-base": "^1.17.1",
    "@opentelemetry/semantic-conventions": "^1.17.1",
    "@ts-rest/core": "^3.30.4",
    "@ts-rest/fastify": "^3.30.4",
    "@ts-rest/open-api": "^3.30.4",
    "bullmq": "^4.12.5",
    "fastify": "^4.24.3",
    "fastify-plugin": "^4.5.1",
    "http-terminator": "^3.2.0",
    "ioredis": "^5.3.2",
    "ms": "^2.1.3",
    "neverthrow": "^6.0.0",
    "pino": "^8.16.0",
    "slonik": "^37.0.1",
    "ts-pattern": "^5.0.5",
    "umzug": "^3.3.1",
    "zod": "^3.22.4"
  }
}
