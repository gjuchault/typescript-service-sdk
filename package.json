{
  "name": "@gjuchault/typescript-service-sdk",
  "version": "0.0.0-development",
  "description": "SDK library for https://github.com/gjuchault/typescript-service-starter",
  "keywords": [
    "typescript",
    "service",
    "sdk"
  ],
  "homepage": "https://github.com/gjuchault/typescript-service-sdk",
  "bugs": "https://github.com/gjuchault/typescript-service-sdk/issues",
  "author": "Gabriel Juchault <gabriel.juchault@gmail.com>",
  "repository": "gjuchault/typescript-service-sdk",
  "type": "module",
  "exports": "./build/index.js",
  "types": "./build/index.d.ts",
  "license": "MIT",
  "engines": {
    "node": "^18.15.0",
    "npm": "^9.5.0"
  },
  "volta": {
    "node": "18.15.0",
    "npm": "9.5.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run clean && npm run type:dts && npm run build:main",
    "build:main": "swc ./src -d ./build",
    "clean": "rimraf build coverage nyc_output",
    "type:dts": "tsc --emitDeclarationOnly --project tsconfig.build.json",
    "type:check": "tsc --noEmit",
    "format": "prettier \"src/**/*.ts\" --write",
    "format:check": "prettier \"src/**/*.ts\" --check",
    "lint": "eslint src --ext .ts --fix",
    "lint:check": "eslint src --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "spell:check": "cspell \"{README.md,CODE_OF_CONDUCT.md,CONTRIBUTING.md,.github/*.md,src/**/*.ts}\"",
    "cz": "cz",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@ryansonshine/commitizen": "^4.2.8",
    "@ryansonshine/cz-conventional-changelog": "^3.3.4",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/npm": "^10.0.0-beta.4",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.42",
    "@types/ms": "^0.7.31",
    "@types/node": "^18.15.5",
    "@types/prompts": "^2.4.3",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "c8": "^7.13.0",
    "cspell": "^6.31.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "nyc": "^15.1.0",
    "prettier": "^2.8.6",
    "rimraf": "^4.4.0",
    "semantic-release": "^21.0.0",
    "source-map-support": "^0.5.21",
    "typescript": "^5.0.2",
    "vitest": "^0.29.8"
  },
  "peerDependencies": {
    "@autotelic/fastify-opentelemetry": "^0.17.1",
    "@fastify/circuit-breaker": "^3.2.0",
    "@fastify/cookie": "^8.3.0",
    "@fastify/cors": "^8.2.1",
    "@fastify/etag": "^4.2.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/helmet": "^10.1.0",
    "@fastify/multipart": "^7.5.0",
    "@fastify/rate-limit": "^8.0.0",
    "@fastify/under-pressure": "^8.2.0",
    "@opentelemetry/api": "^1.2.0",
    "@opentelemetry/api-metrics": "^0.33.0",
    "@opentelemetry/core": "^1.10.0",
    "@opentelemetry/exporter-prometheus": "^0.33.0",
    "@opentelemetry/resources": "^1.10.0",
    "@opentelemetry/sdk-node": "^0.33.0",
    "@opentelemetry/sdk-trace-base": "^1.10.0",
    "@opentelemetry/semantic-conventions": "^1.10.0",
    "@trpc/server": "^10.17.0",
    "bullmq": "^3.10.2",
    "fastify": "^4.15.0",
    "fastify-plugin": "^4.5.0",
    "http-terminator": "^3.2.0",
    "ioredis": "^5.3.1",
    "ms": "^2.1.3",
    "pino": "^8.11.0",
    "slonik": "^33.1.3",
    "ts-brand": "^0.0.2",
    "ts-pattern": "^4.2.1",
    "umzug": "^3.2.1",
    "zod": "^3.21.4"
  },
  "dependencies": {
    "ts-results-es": "^3.6.0"
  }
}
